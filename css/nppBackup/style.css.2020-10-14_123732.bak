a.play_btn {
  font-size: 14px;
  color: white;
  background: #000b337d;
  padding: 2px 10px;
  box-shadow: 3px 4px 8px 1px #1f1e35;
  border-radius: 7%;
  margin: -4px;
  width: 120px;
  display: block;
  text-decoration: none;
  transition:.3s;
  background-position: left center;
  color: #fff;
  opacity: .8;
}

a.play_btn:hover {
  opacity: 1;
  background:#253fea91;
  background-position: right center;
}

